{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaversion": 8,
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "restParams": true,
            "defaultParams": true,
            "spread": true,
            "objectLiteralComputedProperties": true,
            "forOf": true,
            "destructuring": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-app",
        "es-beautifier"
    ],
    "extends": "react-app",
    "rules": {
        "brace-style": 0,
        "linebreak-style": 0,
        "no-console": 0,
        "no-undef": 0,
        "indent": [0, 2],
        "react/jsx-closing-bracket-location": 0,
        "react/jsx-indent-props": [0, 2],
        "react/jsx-indent": [0, 2],
        "react/jsx-no-undef": 0,
        "semi": [2, "never"]
    }
}