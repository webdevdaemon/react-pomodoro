{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"parserOptions": {
		"ecmaversion": 8,
		"ecmaFeatures": {
      "defaultParams": true,
      "destructuring": true,
      "restParams": true,
      "spread": true,
      "arrowFunctions": true,
			"experimentalObjectRestSpread": true,
			"jsx": true
		},
		"sourceType": "module"
	},
	"plugins": [
		"react",
    "es-beautifier"
	],
	"rules": {
		"brace-style": 0,
		"linebreak-style": 0,
		"no-console": 0,
		"no-undef": 0,
	  "indent": [0,2],
	  "react/jsx-closing-bracket-location": "off" ,
	  "react/jsx-indent-props": [0,2],
	  "react/jsx-indent": [0,2],
		"semi": [2, "never"]
	}
}
